filters:
  and:
    - category == "游戏引擎"
views:
  - type: table
    name: 表格
    groupBy:
      property: title
      direction: ASC
    order:
      - file.name
      - file.tags
      - tags
      - description
    sort:
      - property: date
        direction: ASC
    rowHeight: medium
  - type: cards
    name: 视图
    cardSize: 240
    imageFit: contain
    imageAspectRatio: 1.2
  - type: list
    name: List
    markers: number
    indentProperties: true
